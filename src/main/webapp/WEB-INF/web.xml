<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CreatePhoneServlet</servlet-name>
        <servlet-class>com.wcd.excercise5.controller.CreatePhoneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreatePhoneServlet</servlet-name>
        <url-pattern>/addphone</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListPhoneServlet</servlet-name>
        <servlet-class>com.wcd.excercise5.controller.ListPhoneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPhoneServlet</servlet-name>
        <url-pattern>/listphone</url-pattern>
    </servlet-mapping>

</web-app>